<?xml version="1.0" encoding="utf-8"?>
<androidx.wear.widget.BoxInsetLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/box_inset_layout_padding"
    tools:deviceIds="wear"
    android:id="@+id/boxInsetLayout1">

    <navigation
        android:id="@+id/nav_graph"
        android:layout_width="match_parent" >

    </navigation>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/inner_frame_layout_padding"
        app:boxedEdges="all"
        android:id="@+id/frameLayout1"
        android:background="@android:color/holo_blue_bright"
        android:addStatesFromChildren="false"
        android:alwaysDrawnWithCache="false"
        android:animateLayoutChanges="false">

        <Button
            android:text="Ustawienia"
            android:layout_width="160px"
            android:layout_height="100px"
            android:id="@+id/button4"
            android:layout_marginLeft="110px"
            android:layout_marginTop="150px"
            android:textSize="24px"
            android:textStyle="normal"
            android:paddingRight="10px"
            android:paddingLeft="10px"
            android:fadingEdge="none"
            android:layout_gravity="top" >
        </Button>
        <ImageButton
            android:id="@+id/buttonxD"
            android:Source="4kropki.png"
            android:BackgroundColor="Transparent"
            android:WidthRequest="300"
            android:HeightRequest="300"
            android:FlexLayout.AlignSelf="Center"
            android:FlexLayout.Grow="1"
            android:Clicked="ImageButton_Clicked"
            android:Aspect="AspectFit">
        </ImageButton>
    </FrameLayout>
</androidx.wear.widget.BoxInsetLayout>